{
  "random_seed": "range(3)",
  "output_root_dir": "outputs",
  "experiment_id": "exponential_import",
  "save_input_data": false,
  "log_outputs": true,
  "log_time_freq": 10.0,
  "transmission_probabilities": {
    "sporadic": 0.001,
    "transport": 0.01,
    "friendship": 0.1,
    "household": 0.3,
    "workplace": 0.1,
    "constant": 1.25,
    "constant_age": 0.0
  },
  "variable_r_path": "/Users/ub436ne/Workspace/modelling-ncov2019/data/processed/variable_r_test_pl.csv",

  "fear_factors": {
    "default": {
      "fear_function": "fear_disabled"
    }
  },
  "initial_conditions": {
    "selection_algorithm": "random_selection",
    "cardinalities": {
      "immune":  0.01,
      "contraction":  10
    }
  },
  "import_intensity": {
    "function": "exponential",
    "multiplier": 1,
    "rate": 0.14,
    "cap": 1000,
    "infectious": 0.1
  },
  "case_severity_distribution": {
    "asymptomatic": 0.006,
    "mild": 0.809,
    "severe": 0.138,
    "critical": 0.047
  },
  "death_probability": {
    "asymptomatic": 0.0,
    "mild": 0.0,
    "severe": 0.0,
    "critical": 0.49
  },
  "epidemic_status": "not_detected",
  "stop_simulation_threshold": 1000000,
  "stop_simulation_threshold_type": "detections",
  "disease_progression": {
    "default": {
      "t0": {
        "distribution": "from_file",
        "filepath": "$ROOT_DIR/test/models/assets/incubation_period_distribution.npy",
        "approximate_distribution": "lognormal"
      },
      "t1": {
        "distribution": "from_file",
        "filepath": "$ROOT_DIR/test/models/assets/t1_distribution.npy",
        "approximate_distribution": "gamma"
      },
      "t2": {
        "distribution": "from_file",
        "filepath": "$ROOT_DIR/test/models/assets/t1_t2_distribution.npy",
        "approximate_distribution": "gamma"
      },
      "tdeath": {
        "distribution": "from_file",
        "filepath": "$ROOT_DIR/test/models/assets/onset_death_distribution.npy",
        "approximate_distribution": "lognormal"
      }
    }
  }
}